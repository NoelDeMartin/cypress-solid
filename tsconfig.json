{
    "compilerOptions": {
        "target": "esnext",
        "module": "esnext",
        "sourceMap": true,
        "skipLibCheck": true,
        "moduleResolution": "node",
        "strict": true,
        "types": ["cypress", "node", "jest"],
        "noUncheckedIndexedAccess": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "lib": ["esnext"],
        "baseUrl": "./src",
        "paths": {
            "@/*": ["./*"]
        }
    }
}
